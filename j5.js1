function setup() {
  createCanvas(400, 600);
  //el 치rea de trabajo 400 es la base y 600 la altura
}

function draw() {
  
  fill(245) //Este es el color del rect치ngulo de arriba
  rect(0, 0, 400, 200); // Esta es la posici칩n del rect치ngulo. 
  
  fill (143,13, 39)
